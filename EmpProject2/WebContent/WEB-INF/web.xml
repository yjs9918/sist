<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EmpProject2</display-name>
  <servlet>
    <servlet-name>main</servlet-name>
    <servlet-class>com.sist.view.MainServlet</servlet-class>
  </servlet>
  <!-- 톰캣이 읽어가는 파일 -->
  <servlet-mapping>
    <servlet-name>main</servlet-name>
    <url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.sist.view.LoginServlet</servlet-class>
  </servlet>
  <!-- 톰캣이 읽어가는 파일 -->
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>header</servlet-name>
    <servlet-class>com.sist.view.HeaderServlet</servlet-class>
  </servlet>
  <!-- 톰캣이 읽어가는 파일 -->
  <servlet-mapping>
    <servlet-name>header</servlet-name>
    <url-pattern>/HeaderServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>